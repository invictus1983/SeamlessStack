<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
                     http://clish.sourceforge.net/XMLSchema/clish.xsd">
    <!--=======================================================-->
	<PTYPE name="VLAN_ID" 
        pattern="(409[0-5]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{2}|[1-9][0-9]|[1-9])"
           help="Number in the range 1-4095"/>
	<!--=======================================================-->
	<PTYPE name="IP_ADDR"
	    pattern="(((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))"
	       help="IP address AAA.BBB.CCC.DDD where each part is in the range 0-255"/>
	<!--=======================================================-->
	<PTYPE name="UINT"
	    pattern="[0-9]+"
	       help="Unsigned integer"/>
	<!--=======================================================-->
	<PTYPE name="STRING"
	    pattern="[^\-]+"
	       help="String"/>
	<!--=======================================================-->
	<PTYPE name="BOOL"
	    pattern="true(1) false(0)"
	       help="Boolean choice"/>
	<!--=======================================================-->
	<PTYPE name="PROTOCOL"
		pattern="NFS(1) ISCSI(2) IB(3)"
		help="Protocol"/>
	<!--=======================================================-->
	<PTYPE name="STYPE"
		pattern="HDD(1) SSD(2)"
		help="Storage media type"/>
	<!--=======================================================-->
	<PTYPE name="QTYPE"
		pattern="high(2) medium(1) low(0)"
		help="Quality of Service"/>
	<!--=======================================================-->
	<PTYPE name="PLUGIN"
		pattern="none(0) compression(1) encryption(2) snapshot(3) dedup(4)"
		help="Storage plugin types"/>
	<!--=======================================================-->
</CLISH_MODULE>
